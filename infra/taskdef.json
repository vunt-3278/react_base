{
    "executionRoleArn": "$executionRoleArn",
    "taskRoleArn": "$taskRoleArn",
    "containerDefinitions": [
        {
            "name": "$CONTAINER_NAME",
            "image": "$REPOSITORY_URI:$IMAGE_TAG",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": "$CONTAINER_PORT",
                    "hostPort": "$HOST_PORT"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "memory": "$TASK_MEMORY",
    "cpu": "$TASK_CPU",
    "family": "$TASK_FAMILY"
}